Algoritmo sin_titulo		
	Dimension vect1[20] //para las notas
	Dimension vect2[50] //para los estud
	Para i<-1 Hasta 5 Hacer
		vect1[i]<-Aleatorio(0,20);
		Escribir vect1[i]
	FinPara
	Para i<-1 Hasta 5 hacer
		Si vect1[i]>0 y vect1[i]<5 Entonces
			Escribir "Para el Alumno n°",i;
			Escribir "La nota ",i," es : Deficiente";
		Fin Si
		Si vect1[i]>6 y vect1[i]<10 Entonces
			Escribir "Para el Alumno n°",i;
			Escribir "La nota ",i," es : Regular";
		FinSi
		Si vect1[i]>11 y vect1[i]<15 Entonces
			Escribir "Para el Alumno n°",i;
			Escribir "La nota ",i," es : Bueno";
		FinSi
		Si vect1[i]>16 y vect1[i]<20 Entonces	
			Escribir "La nota ",i," es : Excelente";
		FinSi
	FinPara
	
	
	
FinAlgoritmo
