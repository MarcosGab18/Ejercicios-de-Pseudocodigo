Proceso sin_titulo
	//La suma de los N primeros numeros naturales
	//El valor de N se leera por teclado.
	Definir n,i,suma Como Real;
	Escribir "Ingresar un numero nartural";
	Leer n;
	suma=0;
	Para i<-1 Hasta n Hacer
		suma=suma+i;
	FinPara
	Escribir "La suma de los primeros numeros naturales es ", suma;
	
FinProceso
